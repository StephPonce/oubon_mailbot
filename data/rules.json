{
  "rules": [
    {
      "if_any": [
        "refund",
        "return",
        "exchange",
        "money back",
        "cancel order",
        "don't want",
        "changed my mind"
      ],
      "apply_label": "Support",
      "auto_reply_template": "refund_request",
      "auto_reply": true
    },
    {
      "if_any": [
        "broken",
        "damaged",
        "defective",
        "wrong item",
        "missing parts",
        "not working",
        "quality issue",
        "not as described"
      ],
      "apply_label": "Support",
      "auto_reply_template": "support_quality",
      "auto_reply": true
    },
    {
      "if_any": [
        "tracking",
        "where is",
        "shipment",
        "hasn't arrived",
        "hasnt arrived",
        "not arrived",
        "missing",
        "unreceived",
        "not received",
        "delayed",
        "late"
      ],
      "apply_label": "Orders",
      "auto_reply_template": "tracking_lookup",
      "auto_reply": true
    },
    {
      "if_any": [
        "order",
        "package",
        "delivery",
        "shipping",
        "order status",
        "order update"
      ],
      "apply_label": "Orders",
      "auto_reply_template": "order_help",
      "auto_reply": true
    },
    {
      "if_any": [
        "invoice",
        "receipt",
        "charge",
        "payment",
        "credit card",
        "billing",
        "overcharged",
        "double charged"
      ],
      "apply_label": "Billing",
      "auto_reply_template": "billing_help",
      "auto_reply": true
    },
    {
      "if_any": [
        "unsubscribe",
        "stop emails",
        "remove me",
        "opt out",
        "newsletter",
        "promotional"
      ],
      "apply_label": "Marketing",
      "auto_reply_template": null,
      "auto_reply": false
    },
    {
      "if_any": [
        "press",
        "investor",
        "wholesale",
        "bulk order",
        "partnership",
        "collaboration",
        "media"
      ],
      "apply_label": "VIP",
      "auto_reply_template": "vip_welcome",
      "auto_reply": true
    },
    {
      "if_any": [
        "mailer-daemon",
        "delivery status notification",
        "failure notice",
        "postmaster",
        "no-reply",
        "do-not-reply",
        "noreply"
      ],
      "apply_label": "Admin",
      "auto_reply_template": null,
      "auto_reply": false
    },
    {
      "if_any": [
        "revenue",
        "profit",
        "sales data",
        "how much do you make",
        "supplier",
        "dropshipping",
        "aliexpress",
        "other customers",
        "customer list"
      ],
      "apply_label": "Auto Ignored",
      "auto_reply_template": null,
      "auto_reply": false
    }
  ]
}